{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ES2020",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "esModuleInterop": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": ["@types/babel__core"]
  },
  "include": ["vite.config.ts", "**/*.ts", "**/*.tsx", "src/**/*"]
}
